<p id="notice"><%= notice %></p>

<h1>Applicants</h1>

<% @app_infos.each do |app_info| %>
  <div class="row">
    <div class="col s12 m7">
      <div class="card">
        <div class="card-image">
          <img src="images/sample-1.jpg">
          <span class="card-title">	<%= app_info.first_name %></span>
        </div>
        <div class="card-content">
			<%= app_info.email %>
			<%= app_info.developer_type %>
			<%= app_info.hometown %>
        </div>
      </div>
    </div>
  </div>
<% end %>

<a class="btn-floating btn-large waves-effect waves-light red"><i class="material-icons">add fellow</i></a>

<br>
