<p id="notice"><%= notice %></p>

<div align="center">
<p>
  <strong>Applicant Profile</strong>
  <% for attribute in @app_info.attributes.keys %>
	<p><%= attribute.humanize %> : <%= @app_info.attributes[attribute].to_s %></p>
  <% end %>
</p>

<%= button_to 'destroy',@app_info, method: :delete, :onclick => "return confirm('Are you sure you want to delete this post?')" %>
<%= link_to 'Back', app_infos_path %>
</div>